{
  "name": "advisory-generator",
  "version": "1.0.0",
  "description": "Service: Advisory Generator Programming Lanuage: NodeJS/Express",
  "main": "index.js",
  "scripts": {
    "docker_build": "docker build -t kagarcera/advisory . && docker push kagarcera/advisory:latest",
    "kubectl_restart": "kubectl rollout restart deployment advisory-depl",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "cd ./app && sequelize db:migrate",
    "seed": "cd ./app && sequelize db:seed:all",
    "dev": "nodemon server.js",
    "prod": "forever -a server.js",
    "email": "forever start -a script.job.email.js",
    "queue": "forever start -a script.job.queue.js",
    "list": "forever list",
    "stop_server": "forever stop server.js",
    "stop_email": "forever stop script.job.email.js",
    "stop_queue": "forever stop script.job.queue.js",
    "logs": "forever logs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "forever": "^3.0.4",
    "fs": "0.0.1-security",
    "handlebars": "^4.7.7",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.5.0",
    "nodemailer-plugin-inline-base64": "^2.1.1",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "request-ip": "^2.1.3",
    "sequelize": "^6.5.1",
    "sequelize-cli": "^6.2.0"
  }
}
